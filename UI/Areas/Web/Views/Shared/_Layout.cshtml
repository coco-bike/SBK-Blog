<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>@ViewBag.Title</title>
    <link href="https://cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="~/Content/Plugs/Wow/css/libs/animate.css" rel="stylesheet" />
    <link href="~/Areas/Web/Css/Layout/layout.css" rel="stylesheet" />
    @RenderSection("MySection", required: false)
    <script src="http://www.jq22.com/jquery/jquery-1.10.2.js"></script>
</head>
<body>
    <div class="header" id="demo">
        <div class="wow rollIn">
            <div class="title">
                <p>SongBiKe网络博客</p>
            </div>
        </div>
        <div class="wow bounceInUp">
            @RenderBody()
        </div>
        <div class="canvaszz"> </div>
        <canvas id="canvas"></canvas>
    </div>
    <!--用来解决视频右键菜单，用于视频上面的遮罩层 START-->
    <div class="videozz"></div>
    <!--用来解决视频右键菜单，用于视频上面的遮罩层 END-->
    <!--音乐 START-->
    <audio controls autoplay class="audio">
        <source src="~/Content/Plugs/3DStart/Music.mp3" type="audio/mp3">
        <source src="css/Music.ogg" type="audio/ogg">
        <source src="css/Music.aac" type="audio/mp4">
    </audio>
    <!--音乐 END-->

    <script src="https://cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="~/Content/Plugs/Wow/dist/wow.min.js"></script>
    <script src="~/Content/Plugs/3DStart/xingkong.js"></script>
    @RenderSection("MySection1", required: false)
    <script>
        new WOW().init();
    </script>
</body>
</html>